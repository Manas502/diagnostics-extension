{
    "manifest_version": 2,
    "name": "System Diagnostics",
    "description": "This extension will do system diagnostics",
    "permissions": ["system.cpu"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "page_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon-images/images/get_started16.png",
            "32": "icon-images/images/get_started32.png",
            "48": "icon-images/images/get_started48.png",
            "128": "icon-images/images/get_started128.png"
          }
      },
      "icons": {
        "16": "icon-images/images/get_started16.png",
        "32": "icon-images/images/get_started32.png",
        "48": "icon-images/images/get_started48.png",
        "128": "icon-images/images/get_started128.png"
      },
    "version": "1.0"
  }